{
  "name": "DD",
  "language": ["en"],
  "domains": ["rapidmoviez.me"],
  "base_url": "https://rapidmoviez.me",
  "search_url_format_episode": {
    "string_format": "/search/%s+S%02dE%02d/all/exact",
    "replacement": [
      "title",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/search/%s+%04d/all/exact",
    "replacement": [
      "title",
      "year_int"
    ]
  },
  "more_pages_indicator": "",
  "next_page_path": "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "div.list_items ul li a.title[href*=/release/]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": false,
  "is_direct": true,
  "links_container_path": "div[style*='position:relative']",
  "link_path": "pre.links",
  "link_path_attr": "text",
  "title_path": "/title",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "",
  "magnet_path_attr": "",
  "seeders_path": "",
  "seeders_path_attr": "",
  "filesize_path": "p:contains('size:')",
  "filesize_path_attr": "",
  "quality_path": "|link_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
