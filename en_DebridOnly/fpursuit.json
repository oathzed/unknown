{
  "name": "FPursuit",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "filepursuit.com"
  ],
  "base_url": "https://filepursuit.com",
  "search_url_format_episode": {
    "string_format": "/pursuit?q=%s+S%02dE%02d&type=video&sort=datedesc",
    "replacement": [
      "title",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/pursuit?q=%s+%04d&type=video&sort=datedesc",
    "replacement": [
      "title",
      "year_int"
    ]
  },
  "more_pages_indicator": "",
  "next_page_path": "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "div.row div.mb-4.mb-md-0.mr-6 a[href^=/file]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": false,
  "is_direct": true,
  "links_container_path": "div",
  "link_path": "a",
  "link_path_attr": "href",
  "title_path": "/title",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "",
  "magnet_path_attr": "",
  "seeders_path": "",
  "seeders_path_attr": "",
  "filesize_path": "p:contains('size:')",
  "filesize_path_attr": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
