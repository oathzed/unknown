{
  "name": "torrent9",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "torrent9.se"
  ],
  "base_url": "https://torrent9.se",
  "search_url_format_episode": {
    "string_format": "/search_torrent/%s-s%02de%02d.html",
    "replacement": [
      "title_lower_dash",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/search_torrent/%s+%04d.html",
    "replacement": [
      "title_lower_dash",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/search_torrent/%s.html",
    "replacement": [
      "title_lower_dash"
    ]
  },
  "more_pages_indicator": "",
  "next_page_path": "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "tbody tr td a[href^=/torrent]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.download-btn",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "/title",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "a[href^=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "td:eq(4) span.red",
  "seeders_path_attr": "",
  "filesize_path": "td:eq(3)",
  "filesize_path_attr": "",
  "filesize_units": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
