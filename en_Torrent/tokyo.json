{
	"name": "TokyoT",
	"language": ["en"],
	"domains": ["tokyotosho.info"],
	"base_url": "https://tokyotosho.info",
	"search_url_format_episode": {
		"string_format": "/search.php?terms=%s+S%02dE%02d&type=0&searchName=true&searchComment=true&size_min=&size_max=&username=",
		"replacement": [
			"title",
			"season_int",
			"episode_int"
		]

	},

	"search_url_format_movie": {
		"string_format": "/search.php?terms=%s+%04d&type=0&searchName=true&searchComment=true&size_min=&size_max=&username=",
		"replacement": [
			"title",
			"year_int"
		]
	},
	"search_url_format_season_pack": {
		"string_format": "/search.php?terms=%s&type=0&searchName=true&searchComment=true&size_min=&size_max=&username=",
		"replacement": [
			"title"
		]
	},
	"more_pages_indicator": "",
	"next_page_path": "",
	"next_page_path_attr": "",
	"links_on_first_page": true,
	"drill_down_path": "",
	"drill_down_path_attr": "",
	"is_torrent": true,
	"is_direct": false,
	"links_container_path": "table.torrent-list tbody tr",
	"link_path": "",
	"link_path_attr": "",
	"title_path": "td:eq(1) a[title]",
	"title_path_attr": "",
	"details_path": "|title_path|",
	"details_path_attr": "",
	"magnet_path": "td:eq(2) a[href^=magnet]",
	"magnet_path_attr": "href",
	"seeders_path": "td:eq(5)",
	"seeders_path_attr": "",
	"filesize_path": "td:eq(3)",
	"filesize_path_attr": "",
	"filesize_units": "",
	"quality_path": "|title_path|",
	"quality_path_attr": "",
	"info_path": "|title_path|",
	"info_path_attr": ""
}
