{
  "name": "torrentfunk",
  "language": [
    "hi",
    "en"
  ],
  "domains": [
    "torrentfunk..net"
  ],
  "base_url": "https://torrentfunk.net",
  "search_url_format_episode": {
    "string_format": "/torrents?q=%s+s%02de%02d",
    "replacement": [
      "title",
      "season_int",
      "episode_int"
    ]
  },
  "search_url_format_movie": {
    "string_format": "/torrents?q=%s+%04d",
    "replacement": [
      "title",
      "year_int"
    ]
  },
  "search_url_format_season_pack": {
    "string_format": "/torrents?q=%s",
    "replacement": [
      "title"
    ]
  },
  "more_pages_indicator": "",
  "next_page_path": "",
  "next_page_path_attr": "",
  "links_on_first_page": false,
  "drill_down_path": "div.mx-4.mt-4.space-y-4 a[href^=/torrents]",
  "drill_down_path_attr": "abs:href",
  "is_torrent": true,
  "is_direct": false,
  "links_container_path": "div.flex.lg:block.lg:space-y-2.card-actions",
  "link_path": "",
  "link_path_attr": "",
  "title_path": "/title",
  "title_path_attr": "",
  "details_path": "|title_path|",
  "details_path_attr": "",
  "magnet_path": "a[href^=magnet]",
  "magnet_path_attr": "href",
  "seeders_path": "td:eq(4) span.red",
  "seeders_path_attr": "",
  "filesize_path": "td:eq(3)",
  "filesize_path_attr": "",
  "filesize_units": "",
  "quality_path": "|title_path|",
  "quality_path_attr": "",
  "info_path": "|title_path|",
  "info_path_attr": ""
}
